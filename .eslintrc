{
  "extends": [
    "standard",
    "plugin:prettier/recommended",
    "prettier",
    "prettier/standard",
    "plugin:jest/recommended"
  ],
  "plugins": [
    "standard",
    "jest"
  ],
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "jasmine": true,
    "jest/globals": true
  },
  "rules": {
    "semi": [2, "never"],
    "no-unused-expressions": 0
  }
}
